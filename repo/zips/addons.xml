        <dir>
            <info compressed="false">https://raw.githubusercontent.com/CheapCyborg/Cyborgs-AH2/master/repo/zips/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/CheapCyborg/Cyborgs-AH2/master/repo/zips/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/CheapCyborg/Cyborgs-AH2/master/repo/zips/</datadir>
        </dir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>Cyborg's Repo</summary>
        <description>Repo for CyborgWizard</description>
        <disclaimer />
        <platform>all</platform>
        <assets>
            <icon>icon.png</icon>
            <fanart>fanart.jpg</fanart>
        </assets>
    </extension>
</addon></addons>